{
    "name": "samsin33/laravel-decision-engine",
    "description": "This package provide support for making decision engine in laravel.",
    "keywords": ["laravel", "decision engine", "rule engine"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/samsin33/laravel-decision-engine/issues",
        "source": "https://github.com/samsin33/laravel-decision-engine"
    },
    "autoload": {
        "psr-4": {
            "Samarth\\DecisionEngine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Samarth\\DecisionEngine\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Samarth Sinha",
            "email": "samarthasinha09@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/log": "^9.0",
        "illuminate/notifications": "^9.0",
        "illuminate/routing": "^9.0",
        "illuminate/view": "^9.0",
        "illuminate/console": "^9.0",
        "illuminate/container": "^9.0",
        "illuminate/contracts": "^9.0",
        "illuminate/cookie": "^9.0",
        "illuminate/database": "^9.0",
        "illuminate/encryption": "^9.0",
        "illuminate/http": "^9.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Samsin33\\DecisionEngine\\Providers\\DecisionEngineServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
